{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended"],
    "jsRules": {},
    "rules": {
      "quotemark": [true, "single", "avoid-escape"],
      "no-console": {
        "severity": "warning",
        "options": ["log", "debug", "info", "time", "timeEnd", "trace"]
      },
      "trailing-comma": [true, { "multiline": { "arguments": "never" } }]
    },
    "rulesDirectory": []
  }
  